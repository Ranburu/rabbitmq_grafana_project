global:
  scrape_interval: 10s

scrape_configs:
  - job_name: 'prometheus_master'
    scrape_interval: 5s
    static_configs:
      - targets: ['{{ prometheus_hosts }}:{{ prometheus_port }}']
  - job_name: 'node_exporter'
    scrape_interval: 5s
    static_configs:
      - targets: ['{{ prometheus_rabbitmq_hosts }}:{{ prometheus_node_exporter_port }}']
  - job_name: 'rabbitmq_exporter'
    scrape_interval: 5s
    static_configs:
      - targets: ['{{ prometheus_rabbitmq_hosts }}:{{ prometheus_rabbitmq_exporter_port }}']